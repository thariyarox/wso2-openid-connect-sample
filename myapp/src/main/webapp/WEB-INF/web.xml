<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>OAuthCallback</servlet-name>
        <servlet-class>com.verizon.bda.oauth2.testsuite.servlet.OAuthCallback</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>OAuthCallback</servlet-name>
        <url-pattern>/callback</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>TokenRequest</servlet-name>
        <servlet-class>com.verizon.bda.oauth2.testsuite.servlet.TokenRequest</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TokenRequest</servlet-name>
        <url-pattern>/tokenrequest</url-pattern>
    </servlet-mapping>


</web-app>
